{
	"content_scripts" : [{
	"js" : [
		"javascripts/jquery-1.9.1.min.js",
		"javascripts/deserialize.js",
		"javascripts/content_script.js",
		"javascripts/mousetrap.min.js"
	],
	"matches" : [ "http://*/*", "https://*/*", "file:///*" ],
	"run_at" : "document_start"
	}],
	"name": "DWD Form Filler",
	"version": "1.1.0",
	"icons" : {
		"16" :  "images/16.png",
	  "48" :  "images/48.png",
	  "128" : "images/128.png"},
	"description": "Useful for developers to fill forms.",
	"browser_action": {
		"default_icon": "images/48.png",
		"default_title": "DWD Form Filler",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"tabs",
		"http://*/", "https://*/",
		"storage"
	],
	"manifest_version": 2
}
